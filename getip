# find machine ip address within LAN
OS=$(uname)
if [ "$OS" = "Linux" ]; then
    ip=$(hostname -I | xargs)
    echo '\n':::::   Connection Code: $ip:8000   :::::'\n'> ./connectedhealth/ip.txt
elif [ "$OS" = "Darwin" ]; then
    ip=$(ipconfig getifaddr en0)
    echo '\n':::::   Connection Code = $ip:8000   :::::'\n'> ./connectedhealth/ip.txt
fi